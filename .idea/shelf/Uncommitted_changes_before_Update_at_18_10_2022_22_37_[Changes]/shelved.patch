@@ -0,0 +1,15 @@
+package DR3;
+
+public class Ex2_5 {
+    public static void main(String[] args) {
+        byte a = (byte) 130;
+        byte s = (byte) 1;
+        System.out.println(left(a , s));
+    }
+
+    private static byte left(byte a, byte s) {
+        int x = a & 0xFF;
+        int y = s % 8;
+        return (byte) ((x << y) | (x >> (8 - y)));
+    }
+}
